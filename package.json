{
  "name": "dpl-syntax-highlighting",
  "displayName": "DPL-syntax-highlighting",
  "description": "Syntax highlighter for the Dubious Programming Language (DPL)",
  "version": "0.0.6",
  "publisher": "Aurélien Bück-Kaeffer",
  "repository": "https://github.com/Scezaquer/DPL-syntax-highlighting",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "dpl",
    "dubious programming language",
    "syntax",
    "highlighting"
  ],
  "license": "MIT",
  "contributes": {
    "languages": [{
      "id": "dpl",
      "aliases": ["DPL", "dpl"],
      "extensions": [".dpl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "dpl",
      "scopeName": "source.dpl",
      "path": "./syntaxes/dpl.tmLanguage.json"
    }]
  }
}
